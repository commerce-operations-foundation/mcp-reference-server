{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Inventory",
  "description": "Inventory Item",
  "type": "object",
  "properties": {
    "available": {
      "type": "number"
    },
    "availableToPromise": {
      "type": "number"
    },
    "commitShip": {
      "type": "number"
    },
    "commitXfer": {
      "type": "number"
    },
    "committed": {
      "type": "number"
    },
    "committedFuture": {
      "type": "number"
    },
    "damaged": {
      "type": "number"
    },
    "future": {
      "type": "number"
    },
    "hold": {
      "type": "number"
    },
    "incoming": {
      "type": "number"
    },
    "inventoryNotTracked": {
      "type": "boolean"
    },
    "label": {
      "type": "string"
    },
    "locationId": {
      "type": "string"
    },
    "onHand": {
      "type": "number"
    },
    "quantity": {
      "type": "number"
    },
    "sku": {
      "type": "string"
    },
    "unavailable": {
      "type": "number"
    },
    "id": {
      "description": "Unique system-generated identifier for this entity (read-only)",
      "type": "string"
    },
    "externalId": {
      "description": "ID of the entity in the client's system. Must be unique within the tenant.",
      "type": "string"
    },
    "createdAt": {
      "description": "ISO 8601 timestamp when the entity was created (read-only)",
      "type": "string",
      "format": "date-time",
      "pattern": "^(?:(?:\\d\\d[2468][048]|\\d\\d[13579][26]|\\d\\d0[48]|[02468][048]00|[13579][26]00)-02-29|\\d{4}-(?:(?:0[13578]|1[02])-(?:0[1-9]|[12]\\d|3[01])|(?:0[469]|11)-(?:0[1-9]|[12]\\d|30)|(?:02)-(?:0[1-9]|1\\d|2[0-8])))T(?:(?:[01]\\d|2[0-3]):[0-5]\\d(?::[0-5]\\d(?:\\.\\d+)?)?(?:Z))$"
    },
    "updatedAt": {
      "description": "ISO 8601 timestamp when the entity was last updated (read-only)",
      "type": "string",
      "format": "date-time",
      "pattern": "^(?:(?:\\d\\d[2468][048]|\\d\\d[13579][26]|\\d\\d0[48]|[02468][048]00|[13579][26]00)-02-29|\\d{4}-(?:(?:0[13578]|1[02])-(?:0[1-9]|[12]\\d|3[01])|(?:0[469]|11)-(?:0[1-9]|[12]\\d|30)|(?:02)-(?:0[1-9]|1\\d|2[0-8])))T(?:(?:[01]\\d|2[0-3]):[0-5]\\d(?::[0-5]\\d(?:\\.\\d+)?)?(?:Z))$"
    },
    "tenantId": {
      "description": "Unique identifier for the tenant that owns this entity (read-only)",
      "type": "string"
    }
  },
  "required": [
    "locationId",
    "sku",
    "id",
    "createdAt",
    "updatedAt",
    "tenantId"
  ],
  "additionalProperties": false
}
